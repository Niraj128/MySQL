SHOW DATABASES;

DROP DATABASE sql_practice;

SELECT DATABASE();


CREATE DATABASE sql_practice;
USE sql_practice;

CREATE TABLE students(

    id INT NOT NULL,
    fname VARCHAR(50),
    mname VARCHAR(50),
    lname VARCHAR(50),
    city VARCHAR(50),
    salary INT
    
);

SHOW TABLES;
DESCRIBE students;

